                <nav>
                    <ul>
                        <li><a href="#" class="link-today">Today</a></li>
                        <li><a href="/notebook">Notebook</a></li>       
                        <!-- <li><a href="#">Donate</a></li>  -->
                    </ul>
                </nav>
            	{% else %}   
